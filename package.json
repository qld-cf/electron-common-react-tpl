{
  "name": "app",
  "description": "{{description}}",
  "author": "{{author}}",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "yarn build-electron && concurrently -n=react,electron -c=blue,green -p=[{name}] \"cross-env BROWSER=none yarn start-react\" \"wait-on http://localhost:3000 && electron .\"",
    "start-react": "node scripts/start.js",
    "build-react": "node scripts/build.js",
    "build-electron": "webpack --config ./config/webpack.electron.config.js",
    "pack": "yarn build-electron && yarn build-react && yarn package",
    "pack-all": "yarn build-electron && yarn build-react && yarn package-all",
    "pack-mac": "yarn build-electron && yarn build-react && electron-builder -m",
    "pack-windows": "yarn build-electron && yarn build-react && electron-builder -w",
    "test": "node scripts/test.js",
    "package": "electron-builder",
    "package-all": "electron-builder -mwl",
    "release": "yarn build-react && electron-builder --publish=always",
    "publish": "build -mwl -p always",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./src"
  },
  "homepage": "./",
  "main": "./electron/build.js",
  "build": {
    "extends": null,
    "productName": "app",
    "appId": "123@qq.com",
    "directories": {
      "output": "release"
    },
    "files": [
      "!node_modules/**/*",
      "build/**/*",
      "electron/build.js",
      "electron/public",
      "electron/public/icon.png"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "artifactName": "${productName}_setup_${version}.${ext}"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:4000/download/"
      }
    ],
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    }
  },
  "dependencies": {
    "electron-is-dev": "^1.1.0",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.2.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.2",
    "@babel/core": "7.7.7",
    "@svgr/webpack": "5.0.1",
    "@types/jest": "24.0.25",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "antd": "^4.0.0",
    "antd-dayjs-webpack-plugin": "0.0.8",
    "axios": "^0.19.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-named-asset-import": "^0.3.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-react-app": "^9.1.0",
    "bfj": "7.0.2",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "3.0.0",
    "classnames": "^2.2.6",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.0",
    "css-loader": "3.4.0",
    "devtron": "^1.4.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.3.2",
    "electron-devtools-installer": "^2.2.4",
    "eslint": "^7.9.0",
    "eslint-config-alloy": "^3.6.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^1.5.0",
    "eslint-plugin-typescript": "^0.14.0",
    "file-loader": "5.0.2",
    "fork-ts-checker-webpack-plugin-alt": "0.4.14",
    "fs-extra": "8.1.0",
    "html-webpack-plugin": "4.0.0-beta.5",
    "identity-obj-proxy": "3.0.0",
    "immutability-helper": "^3.0.1",
    "is-wsl": "^2.1.1",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-pnp-resolver": "1.2.1",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.4.2",
    "js-cookie": "^2.2.1",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-dev-utils": "10.0.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "resolve": "1.14.1",
    "resolve-url-loader": "3.1.1",
    "semver": "7.1.1",
    "source-map-explorer": "^2.2.2",
    "standard": "^14.3.1",
    "style-loader": "1.1.2",
    "terser-webpack-plugin": "2.3.1",
    "ts-pnp": "1.1.5",
    "typescript": "^3.7.4",
    "url-loader": "3.0.0",
    "uuid": "^7.0.1",
    "wait-on": "^4.0.0",
    "webpack": "4.41.5",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "3.10.1",
    "webpack-manifest-plugin": "2.2.0",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "4.3.1",
    "yp-rider": "^1.1.3"
  },
  "resolutions": {
    "react-dev-utils": "10.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd-mobile",
        "style": "css"
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ],
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ]
  ]
}
